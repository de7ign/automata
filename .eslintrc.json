{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "mocha": true
    },
    "extends": ["airbnb", "prettier"],
    "rules": {
        "indent": [2, 2, {"SwitchCase": 1}]
    }
}